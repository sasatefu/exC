module TopWLI (

	 input wire SZeroInput,
    input wire SOneInput,
	 input wire STwoInput,
	 output wire OutA,
	 output wire OutB,
	 output wire OutC,
	 output wire OutD,
	 output wire OutE,
	 output wire OutF,
	 output wire OutG,
);
	 wire SZero;
	 wire SOne;
	 wire STwo;

  	 assign SZero = ~SZeroInput;
	 assign SOne = ~SOneInput;
	 assign STwo = ~STwoInput;
	
	 WLI uut (
        .SZero(SZero),
        .SOne(SOne),
        .STwo(STwo),
        .OutA(OutA),
        .OutB(OutB),
        .OutC(OutC),
        .OutD(OutD),
        .OutE(OutE),
        .OutF(OutF),
        .OutG(OutG)
    );
	 
	 